class Die(object):
    def __init__(self, sides=6):
        self.sides = sides

    def roll(self, qty):
        if qty == 1:
            return random.randint(1, self.sides)
        elif qty > 1:
            return random.randint(1, self.sides) + self.roll(qty - 1)
        else:
            return (0)


